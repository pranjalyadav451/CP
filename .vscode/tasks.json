{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: Windows",
      "command": "C:\\MinGw\\mingw64\\bin\\g++.exe",
      "args": [
        "-std=c++17",
        "-Wall",
        "${file}",
        "-o",
        "a.exe",
        "&&",
        "a.exe<inputf.in>outputf.in"
      ],
      "options": {
        "cwd": "${workspaceRoot}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": "build",
      "detail": "Task generated by Debugger."
    },
    {
      "type": "cppbuild",
      "label": "WSL Build",
      "command": "g++",
      "args": [
        "-std=c++17",
        "${file}",
        "-o",
        "a.exe",
        "&&",
        "timeout",
        "4s",
        "./a.exe<inputf.in>outputf.in"
      ],
      "options": {
        "cwd": "${workspaceRoot}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    },
    {
      "type": "cppbuild",
      "label": "WSL Build - Warn",
      "command": "g++",
      "args": [
        "-std=c++20",
        "-Wshadow",
        "-Wall",
        "-O2",
        "-Wno-unused-result",
        "${file}",
        "-o",
        "main",
        "&&",
        "timeout",
        "4s",
        "./main<inputf.in>outputf.in"
      ],
      "options": {
        "cwd": "${workspaceRoot}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": "build",
      "detail": "Task generated by Debugger."
    },
    {
      "type": "cppbuild",
      "label": "WSL Build -  Debug",
      "command": "g++",
      "args": [
        "-std=c++17",
        "-Wshadow",
        "-Wall",
        "-g",
        "-fsanitize=address",
        "-fsanitize=undefined",
        "${file}",
        "-o",
        "debug",
        "&&",
        "timeout",
        "4s",
        "./debug<inputf.in>outputf.in"
      ],
      "options": {
        "cwd": "${workspaceRoot}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": false
      },
      "detail": "Task generated by Debugger."
    },
  ],
  "version": "2.0.0"
}